prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${prefix}/lib
sharedlibdir=${prefix}/lib
includedir=${prefix}/include

Name: cdroid
Description: C++ style Android UI component library
Version: 1.0.0

Requires:
Libs: -L"${sharedlibdir}" -lcdroid @CDROID_DEPLIBS@
Cflags: -I"${includedir}/gui" -I"${includedir}/porting" -I"@CAIRO_INCLUDE_DIRS@" -I"@FONTCONFIG_INCLUDE_DIRS@"
