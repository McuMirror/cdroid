prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=@prefix@
libdir=@CMAKE_INSTALL_LIBDIR@
includedir=@CMAKE_INSTALL_INCLUDEDIR@

Name: cdroid
Description: Multi-platform 2D graphics library
Version: @CDROID_VERSION@

Requires: cairo fontconfig tvhal
Libs: -L${libdir} -lcdroid
Libs.private: @CDROID_LIBS@
Cflags: -I${includedir}/gui -I${includedir}/porting -I${includedir}/cairo
