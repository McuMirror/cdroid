            </item>
            <item cdroid:duration="15">
                <nine-patch cdroid:src="@mipmap/btn_switch_to_off_mtrl_00009" cdroid:gravity="center" cdroid:tintMode="multiply" cdroid:tint="?attr/colorSwitchThumbNormal" />
            </item>
            <item cdroid:duration="15">
                <nine-patch cdroid:src="@mipmap/btn_switch_to_off_mtrl_00010" cdroid:gravity="center" cdroid:tintMode="multiply" cdroid:tint="?attr/colorSwitchThumbNormal" />
            </item>
            <item cdroid:duration="15">
                <nine-patch cdroid:src="@mipmap/btn_switch_to_off_mtrl_00011" cdroid:gravity="center" cdroid:tintMode="multiply" cdroid:tint="?attr/colorSwitchThumbNormal" />
            </item>
            <item cdroid:duration="15">
                <nine-patch cdroid:src="@mipmap/btn_switch_to_off_mtrl_00012" cdroid:gravity="center" cdroid:tintMode="multiply" cdroid:tint="?attr/colorSwitchThumbNormal" />
            </item>
        </animation-list>
    </transition>
</animated-selector>
