<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_focused="false"
        android:drawable="@mipmap/btn_plus_disable" />
    <item android:state_enabled="false" android:state_focused="true"
        android:drawable="@mipmap/btn_plus_disable_focused" />
    <item android:state_pressed="true" android:drawable="@mipmap/btn_plus_pressed" />
    <item android:state_focused="true" android:state_pressed="false"
        android:drawable="@mipmap/btn_plus_selected" />
    <item android:drawable="@mipmap/btn_plus_default" />
</selector>

