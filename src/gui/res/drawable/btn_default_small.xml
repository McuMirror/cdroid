<selector xmlns:cdroid="http://schemas.android.com/apk/res/android">
    <item cdroid:state_window_focused="false" cdroid:state_enabled="true"
        cdroid:drawable="cdroid:mipmap/btn_default_small_normal" />
    <item cdroid:state_window_focused="false" cdroid:state_enabled="false"
        cdroid:drawable="cdroid:mipmap/btn_default_small_normal_disable" />
    <item cdroid:state_pressed="true" 
        cdroid:drawable="cdroid:mipmap/btn_default_small_pressed" />
    <item cdroid:state_focused="true" cdroid:state_enabled="true"
        cdroid:drawable="cdroid:mipmap/btn_default_small_selected" />
    <item cdroid:state_enabled="true"
        cdroid:drawable="cdroid:mipmap/btn_default_small_normal" />
    <item cdroid:state_focused="true"
        cdroid:drawable="cdroid:mipmap/btn_default_small_normal_disable_focused" />
    <item
         cdroid:drawable="cdroid:mipmap/btn_default_small_normal_disable" />
</selector>
