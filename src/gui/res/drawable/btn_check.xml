<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:cdroid="http://schemas.android.com/apk/res/android">
    <!-- Enabled states -->
        
    <!--item cdroid:state_checked="true" cdroid:state_window_focused="false"
          cdroid:state_enabled="true"
          cdroid:drawable="mipmap/btn_check_ong" />
    <item cdroid:state_checked="false" cdroid:state_window_focused="false"
          cdroid:state_enabled="true"
          cdroid:drawable="mipmap/btn_check_offg" /-->

    <item cdroid:state_checked="true" cdroid:state_pressed="true"
          cdroid:state_enabled="true"
          cdroid:drawable="cdroid:mipmap/btn_check_on_pressed" />
    <item cdroid:state_checked="false" cdroid:state_pressed="true"
          cdroid:state_enabled="true"
          cdroid:drawable="cdroid:mipmap/btn_check_off_pressed" />

    <item cdroid:state_checked="true" cdroid:state_focused="true"
          cdroid:state_enabled="true"
          cdroid:drawable="cdroid:mipmap/btn_check_on_selected" />
    <item cdroid:state_checked="false" cdroid:state_focused="true"
          cdroid:state_enabled="true"
          cdroid:drawable="cdroid:mipmap/btn_check_off_selected" />

    <item cdroid:state_checked="false" cdroid:state_enabled="true"
          cdroid:drawable="cdroid:mipmap/btn_check_off" />
    <item cdroid:state_checked="true"  cdroid:state_enabled="true"
          cdroid:drawable="cdroid:mipmap/btn_check_on" />


    <!-- Disabled states -->

    <item cdroid:state_checked="true" cdroid:state_window_focused="false"
          cdroid:drawable="cdroid:mipmap/btn_check_on_disable" />
    <item cdroid:state_checked="false" cdroid:state_window_focused="false"
          cdroid:drawable="cdroid:mipmap/btn_check_off_disable" />

    <item cdroid:state_checked="true" cdroid:state_focused="true"
          cdroid:drawable="cdroid:mipmap/btn_check_on_disable_focused" />
    <item cdroid:state_checked="false" cdroid:state_focused="true"
          cdroid:drawable="cdroid:mipmap/btn_check_off_disable_focused" />

    <item cdroid:state_checked="false" cdroid:drawable="cdroid:mipmap/btn_check_off_disable" />
    <item cdroid:state_checked="true" cdroid:drawable="cdroid:mipmap/btn_check_on_disable" />

</selector>
