<theme name="default">
	<pattern name="select_1" type="linear" x0="0" y0="0" x1="200" y1="30">
		<colorstop offset="0" color="0x88666666"/>
		<colorstop offset="0.5" color="0xff888888"/>
		<colorstop offset="1" color="0x88666666"/>
	</pattern>
	<style name="hscrollbar" height="10">
		<item name="track"></item>
		<item name="thumb"></item>
		<item name="corner"></item>
		<item name="leftbutton"></item>
		<item name="rightbutton"></item>
		<item name="up"></item>
	</style>
	<style name="view" border="true">
		<item name="background" focused="true">0xff444444</item>
		<item name="background" focused="false">0xff444444</item>
		<item name="textcolor" focused="true">0xff123456</item>
		<item name="textcolor" focused="false">0xff123456</item>
		<item name="border" focused="true">0xffff0000</item>
		<item name="fontsize" focused="true">24</item>
		<item name="fontsize" focused="true">24</item>
	</style>
	<style name="listview" border="true" parent="view">
		<item name="selected" focused="true" >selected_1</item>
		<item name="selected" focused="false">0x44880000</item>
	</style>
</theme>
